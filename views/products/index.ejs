<!-- views/products/index.ejs -->
<%- include('../partials/header') %>
<table id="productList">
  <tbody>
    <% products.forEach(function(p) { %>
      <tr>
        <td><%= p.name %></td>
        <td><%= p.price %></td>
        <td><a href="">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
<%- include('../partials/footer') %>